[32m+[m[32m<script>[m
[32m+[m[32m    document.addEventListener("DOMContentLoaded", function() {[m
[32m+[m[32m      const toggleBtn = document.getElementById("formToggle");[m
[32m+[m[32m      const submenu = document.getElementById("submenuForm");[m
[32m+[m[32m      if (toggleBtn && submenu) {[m
[32m+[m[32m        toggleBtn.addEventListener("click", function(e) {[m
[32m+[m[32m          e.preventDefault();[m
[32m+[m[32m          submenu.classList.toggle("show");[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m</script>[m
[32m+[m
[32m+[m[32m<script>[m
[32m+[m[32m    document.addEventListener("DOMContentLoaded", function() {[m
[32m+[m[32m      function actualizarNotificaciones() {[m
[32m+[m[32m        fetch("../PHP/NOTIFICACIONES/contar_notis.php")[m
[32m+[m[32m          .then(response => response.text())[m
[32m+[m[32m          .then(data => {[m
[32m+[m[32m            const noti = document.getElementById("notiCount");[m
[32m+[m[32m            if (noti) {[m
[32m+[m[32m              noti.textContent = data;[m
[32m+[m[32m              if (parseInt(data) === 0) {[m
[32m+[m[32m                noti.style.display = "none";[m
[32m+[m[32m              } else {[m
[32m+[m[32m                noti.style.display = "inline-block";[m
[32m+[m[32m              }[m
[32m+[m[32m            }[m
[32m+[m[32m          })[m
[32m+[m[32m          .catch(error => console.error("Error cargando notificaciones:", error));[m
[32m+[m[32m      }[m
[32m+[m[32m      actualizarNotificaciones();[m
[32m+[m[32m      setInterval(actualizarNotificaciones, 10000);[m
[32m+[m[32m    });[m
[32m+[m[32m</script>[m
[32m+[m
[32m+[m[32m<!-- Chart.js -->[m
[32m+[m[32m<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>[m
[32m+[m[32m<script>[m
[32m+[m[32m    document.addEventListener('DOMContentLoaded', () => {[m
[32m+[m[32m      const ventasCtx = document.getElementById('ventasChart').getContext('2d');[m
[32m+[m[32m      const ventasChart = new Chart(ventasCtx, {[m
[32m+[m[32m        type: 'line',[m
[32m+[m[32m        data: {[m
[32m+[m[32m          labels: ['Ene','Feb','Mar','Abr','May','Jun'],[m
[32m+[m[32m          datasets:[{[m
[32m+[m[32m            label:'Ventas',[m
[32m+[m[32m            data:[12,19,15,25,22,30],[m
[32m+[m[32m            borderColor:'#5c0b26',[m
[32m+[m[32m            backgroundColor:'rgba(92,11,38,0.15)',[m
[32m+[m[32m            tension:0.3,[m
[32m+[m[32m            fill:true[m
[32m+[m[32m          }][m
[32m+[m[32m        },[m
[32m+[m[32m        options: { responsive: true, maintainAspectRatio: false }[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      const usuariosCtx = document.getElementById('usuariosChart').getContext('2d');[m
[32m+[m[32m      const usuariosChart = new Chart(usuariosCtx, {[m
[32m+[m[32m        type: 'pie',[m
[32m+[m[32m        data: {[m
[32m+[m[32m          labels:['Activos','Inactivos'],[m
[32m+[m[32m          datasets:[{[m
[32m+[m[32m            data:[100,35],[m
[32m+[m[32m            backgroundColor:['#5c0b26','#d6a5b1'][m
[32m+[m[32m          }][m
[32m+[m[32m        },[m
[32m+[m[32m        options: { responsive: true, maintainAspectRatio: false }[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      const ingresosCtx = document.getElementById('ingresosChart').getContext('2d');[m
[32m+[m[32m      const ingresosChart = new Chart(ingresosCtx, {[m
[32m+[m[32m        type: 'bar',[m
[32m+[m[32m        data: {[m
[32m+[m[32m          labels:['Ene','Feb','Mar','Abr','May','Jun'],[m
[32m+[m[32m          datasets:[{[m
