[32m+[m[32m        </tr>[m
[32m+[m[32m        <?php } ?>[m
[32m+[m[32m        <?php } else { ?>[m
[32m+[m[32m        <tr><td colspan="5">No se encontraron resultados</td></tr>[m
[32m+[m[32m        <?php } ?>[m
[32m+[m[32m        </tbody>[m
[32m+[m[32m    </table>[m
[32m+[m[32m</div>[m
[32m+[m
[32m+[m[32m<!---------JAVA SCRIPT----->[m
[32m+[m[32m<!----------CALCULO-------------->[m
[32m+[m[32m<script>[m
[32m+[m[32m    const subTotal = document.getElementById("SubTotal");[m
[32m+[m[32m    const iva = document.getElementById("Iva");[m
[32m+[m[32m    const total = document.getElementById("Total");[m
[32m+[m
[32m+[m[32m    function calcularTotal() {[m
[32m+[m[32m    const sub = parseFloat(subTotal.value) || 0;[m
[32m+[m[32m    const ivaPct = parseFloat(iva.value) || 0;[m
[32m+[m
[32m+[m[32m    const tot = sub + (sub * ivaPct / 100);[m
[32m+[m
[32m+[m[32m    total.value = tot.toFixed(2); // 2 decimales[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    subTotal.addEventListener("input", calcularTotal);[m
[32m+[m[32m    iva.addEventListener("input", calcularTotal);[m
[32m+[m[32m</script>[m
[32m+[m[32m<!------HEADER----------------->[m
[32m+[m[32m<script>[m
[32m+[m[32m    document.addEventListener("DOMContentLoaded", function() {[m
[32m+[m[32m      const toggleBtn = document.getElementById("formToggle");[m
[32m+[m[32m      const submenu = document.getElementById("submenuForm");[m
[32m+[m
[32m+[m[32m      toggleBtn.addEventListener("click", function(e) {[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[32m        submenu.classList.toggle("show");[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m</script>[m
[32m+[m
[32m+[m[32m</body>[m
[32m+[m[32m</html>[m
[32m+[m[32m>[m
