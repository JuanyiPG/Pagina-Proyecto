[32m+[m[32m          }][m
[32m+[m[32m        },[m
[32m+[m[32m        options: { responsive: true, maintainAspectRatio: false }[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      setInterval(() => {[m
[32m+[m[32m        ventasChart.data.datasets[0].data = Array.from({length:6}, () => Math.floor(Math.random()*40));[m
[32m+[m[32m        ventasChart.update();[m
[32m+[m
[32m+[m[32m        usuariosChart.data.datasets[0].data = [Math.floor(Math.random()*150), Math.floor(Math.random()*60)];[m
[32m+[m[32m        usuariosChart.update();[m
[32m+[m
[32m+[m[32m        ingresosChart.data.datasets[0].data = Array.from({length:6}, () => Math.floor(Math.random()*1600));[m
[32m+[m[32m        ingresosChart.update();[m
[32m+[m[32m      }, 7000);[m
[32m+[m[32m    });[m
[32m+[m[32m</script>[m
