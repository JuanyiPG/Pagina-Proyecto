[32m+[m[32m(async function () {[m
[32m+[m[32m  // ---------------- cargar header ----------------[m
[32m+[m[32m  async function loadHeader() {[m
[32m+[m[32m    try {[m
[32m+[m[32m      const res = await fetch('../Items/header-indexcliente.php');[m
[32m+[m[32m      if (!res.ok) throw new Error('No se pudo cargar header: ' + res.status);[m
[32m+[m[32m      const html = await res.text();[m
[32m+[m[32m      const container = document.getElementById('header-cliente');[m
[32m+[m[32m      if (container) container.innerHTML = html;[m
[32m+[m[32m      return true;[m
[32m+[m[32m    } catch (err) {[m
[32m+[m[32m      console.warn('loadHeader falló:', err);[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // ---------------- inicializadores del header ----------------[m
[32m+[m[32m  function initHeaderInteractions() {[m
[32m+[m[32m    const btnHamb = document.getElementById('btnHamburguesa');[m
[32m+[m[32m    const hambMenu = document.getElementById('hamburgerMenu');[m
[32m+[m[32m    const mainNav = document.getElementById('mainNav');[m
[32m+[m
[32m+[m[32m    if (btnHamb && hambMenu) {[m
[32m+[m[32m      btnHamb.addEventListener('click', (ev) => {[m
[32m+[m[32m        ev.stopPropagation();[m
[32m+[m[32m        hambMenu.classList.toggle('hidden');[m
[32m+[m[32m        hambMenu.setAttribute('aria-hidden', hambMenu.classList.contains('hidden') ? 'true' : 'false');[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      document.addEventListener('click', (e) => {[m
[32m+[m[32m        if (!hambMenu.contains(e.target) && e.target !== btnHamb) {[m
[32m+[m[32m          if (!hambMenu.classList.contains('hidden')) hambMenu.classList.add('hidden');[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (mainNav) {[m
[32m+[m[32m      const onScroll = () => {[m
[32m+[m[32m        if (window.scrollY > 650) mainNav.classList.add('scrolled');[m
[32m+[m[32m        else mainNav.classList.remove('scrolled');[m
[32m+[m[32m      };[m
[32m+[m[32m      window.addEventListener('scroll', onScroll, { passive: true });[m
[32m+[m[32m      onScroll();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    const btnUsuario = document.getElementById('btnUsuario');[m
[32m+[m[32m    const menuUsuario = document.getElementById('menuUsuario');[m
[32m+[m[32m    const btnCerrar = document.getElementById('btnCerrarSesion');[m
[32m+[m
[32m+[m[32m    if (btnUsuario && menuUsuario) {[m
[32m+[m[32m      btnUsuario.addEventListener('click', (ev) => {[m
[32m+[m[32m        ev.stopPropagation();[m
[32m+[m[32m        menuUsuario.classList.toggle('hidden');[m
[32m+[m[32m      });[m
[32m+[m[32m      document.addEventListener('click', (e) => {[m
[32m+[m[32m        if (!menuUsuario.contains(e.target) && e.target !== btnUsuario) {[m
[32m+[m[32m          if (!menuUsuario.classList.contains('hidden')) menuUsuario.classList.add('hidden');[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m    if (btnCerrar) {[m
[32m+[m[32m      btnCerrar.addEventListener('click', () => {[m
[32m+[m[32m        try { localStorage.removeItem('userToken'); } catch (e) {}[m
[32m+[m[32m        window.location.href = '../../index.html';[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // ---------------- inicializador del slider ----------------[m
[32m+[m[32m  function initSlider() {[m
[32m+[m[32m    try {[m
[32m+[m[32m      const slider = document.querySelector('.slider');[m
[32m+[m[32m      const leftBtn = document.querySelector('.arrow.left');[m
[32m+[m[32m      const rightBtn = document.querySelector('.arrow.right');[m
[32m+[m
[32m+[m[32m      if (!slider || !leftBtn || !rightBtn) return;[m
[32m+[m
[32m+[m[32m      const items = slider.querySelectorAll('.item');[m
[32m+[m[32m      if (items.length > 0 && !slider.dataset.duplicated) {[m
[32m+[m[32m        slider.innerHTML += slider.innerHTML;[m
[32m+[m[32m        slider.dataset.duplicated = 'true';[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      const firstItem = slider.querySelector('.item');[m
[32m+[m[32m      const gap = firstItem ? (parseFloat(getComputedStyle(firstItem).marginLeft) + parseFloat(getComputedStyle(firstItem).marginRight)) : 0;[m
[32m+[m[32m      const scrollStep = firstItem ? firstItem.offsetWidth + gap : 250;[m
[32m+[m[32m      let scrollAmount = 0;[m
[32m+[m[32m      const totalWidth = slider.scrollWidth / 2;[m
[32m+[m
[32m+[m[32m      const doNext = () => {[m
[32m+[m[32m        scrollAmount += scrollStep;[m
[32m+[m[32m        if (scrollAmount >= totalWidth) {[m
[32m+[m[32m          scrollAmount = 0;[m
[32m+[m[32m          slider.style.transition = 'none';[m
[32m+[m[32m          slider.style.transform = `translateX(-${scrollAmount}px)`;[m
[32m+[m[32m          void slider.offsetWidth;[m
[32m+[m[32m          slider.style.transition = 'transform 0.5s ease-in-out';[m
[32m+[m[32m        }[m
[32m+[m[32m        slider.style.transform = `translateX(-${scrollAmount}px)`;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      const doPrev = () => {[m
[32m+[m[32m        scrollAmount -= scrollStep;[m
[32m+[m[32m        if (scrollAmount < 0) {[m
[32m+[m[32m          scrollAmount = Math.max(totalWidth - scrollStep, 0);[m
[32m+[m[32m          slider.style.transition = 'none';[m
[32m+[m[32m          slider.style.transform = `translateX(-${scrollAmount}px)`;[m
[32m+[m[32m          void slider.offsetWidth;[m
[32m+[m[32m          slider.style.transition = 'transform 0.5s ease-in-out';[m
[32m+[m[32m        }[m
[32m+[m[32m        slider.style.transform = `translateX(-${scrollAmount}px)`;[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      rightBtn.addEventListener('click', doNext);[m
[32m+[m[32m      leftBtn.addEventListener('click', doPrev);[m
[32m+[m
[32m+[m[32m      let auto = setInterval(doNext, 4000);[m
[32m+[m[32m      slider.addEventListener('mouseenter', () => clearInterval(auto));[m
[32m+[m[32m      slider.addEventListener('mouseleave', () => auto = setInterval(doNext, 4000));[m
[32m+[m[32m    } catch (err) {[m
[32m+[m[32m      console.error('slider init error:', err);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const headerLoaded = await loadHeader();[m
[32m+[m[32m  if (headerLoaded) {[m
[32m+[m[32m    initHeaderInteractions();[m
[32m+[m[32m  } else {[m
[32m+[m[32m    console.warn('Header no cargado: algunos controles del header no funcionarán.');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  initSlider();[m
[32m+[m[32m})();[m
[32m+[m
[32m+[m[32m// ---------------- Add to cart ----------------[m
[32m+[m[32mtry {[m
[32m+[m[32m  const addcarshop = document.getElementById('add-car-shop');[m
[32m+[m[32m  if (addcarshop) {[m
[32m+[m[32m    addcarshop.addEventListener('click', (e) => {[m
[32m+[m[32m      e.preventDefault();[m
[32m+[m[32m      const tallaEl = document.querySelector('input[name="talla"]:checked');[m
[32m+[m[32m      const colorEl = document.querySelector('input[name="color"]:checked');[m
[32m+[m[32m      const talla = tallaEl ? tallaEl.value : 'No seleccionada';[m
[32m+[m[32m      const color = colorEl ? colorEl.value : 'No seleccionada';[m
[32m+[m[32m      alert(`Producto agregado al carrito.\nTalla: ${talla}\nColor: ${color}`);[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m[32m} catch (err) {[m
[32m+[m[32m  console.error('Error al inicializar add-car-shop:', err);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// ---------------- descripción expandible ----------------[m
[32m+[m[32mdocument.addEventListener("DOMContentLoaded", () => {[m
[32m+[m[32m  const headerDesc = document.querySelector('.descripcion-header');[m
[32m+[m[32m  const textoDesc = document.getElementById('descrip-produc');[m
[32m+[m[32m  const arrowDesc = document.querySelector('.arrow-desc');[m
[32m+[m
[32m+[m[32m  if (headerDesc && textoDesc && arrowDesc) {[m
[32m+[m[32m    headerDesc.addEventListener('click', () => {[m
[32m+[m[32m      textoDesc.classList.toggle('show');[m
[32m+[m[32m      textoDesc.classList.toggle('hidden');[m
[32m+[m[32m      arrowDesc.classList.toggle('rotate');[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // ---------------- cambio de colores de imágenes ----------------[m
[32m+[m[32m  const imgprin = document.getElementById('imgprin');[m
[32m+[m[32m  const img1 = document.getElementById('img1');[m
[32m+[m[32m  const img2 = document.getElementById('img2');[m
[32m+[m[32m  const img3 = document.getElementById('img3');[m
[32m+[m
[32m+[m[32m  if (imgprin && img1 && img2 && img3) {[m
[32m+[m[32m    const cambiarImagenes = (imagenes) => {[m
[32m+[m[32m      imgprin.src = imagenes[0];[m
[32m+[m[32m      img1.src = imagenes[1];[m
[32m+[m[32m      img2.src = imagenes[2];[m
[32m+[m[32m      img3.src = imagenes[3];[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    const botonrojo = document.getElementById('color-rojo');[m
[32m+[m[32m    if (botonrojo) {[m
[32m+[m[32m      const imagenroja = [[m
[32m+[m[32m        '../IMG/blusa-hombro-rojo.jpg',[m
[32m+[m[32m        '../IMG/blusa-hombro-rojo.jpg',[m
[32m+[m[32m        '../IMG/blusa-hombro-rojo.jpg',[m
[32m+[m[32m        '../IMG/blusa-hombro-rojo.jpg',[m
[32m+[m[32m      ];[m
[32m+[m[32m      botonrojo.addEventListener('click', () => cambiarImagenes(imagenroja));[m
